{
    "name": "data-visualizer-app",
    "version": "35.13.13",
    "description": "DHIS2 Data Visualizer app",
    "license": "BSD-3-Clause",
    "private": true,
    "homepage": ".",
    "devDependencies": {
        "@dhis2/cli-app-scripts": "^5.2.0",
        "@dhis2/cli-utils-cypress": "^2.2.2",
        "eslint-plugin-cypress": "^2.11.1",
        "redux-mock-store": "^1.5.4"
    },
    "dependencies": {
        "@dhis2/analytics": "^11.1.1",
        "@dhis2/app-runtime": "*",
        "@dhis2/d2-i18n": "*",
        "@dhis2/d2-ui-file-menu": "^7.0.8",
        "@dhis2/d2-ui-interpretations": "^7.0.8",
        "@dhis2/data-visualizer-plugin": "35.13.13",
        "@dhis2/ui": "^5.6.1",
        "@material-ui/icons": "^3.0.1",
        "d2": "31.8.2",
        "history": "^4.7.2",
        "lodash-es": "^4.17.11",
        "prop-types": "^15",
        "raf": "3.4.1",
        "react": "^16.13",
        "react-beautiful-dnd": "^13.0.0",
        "react-dom": "^16.13",
        "react-redux": "^5.1.0",
        "redux": "^4.0.5",
        "redux-actions": "^2.2.1",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0",
        "reselect": "^4.0.0",
        "whatwg-fetch": "^3.4.1"
    },
    "scripts": {
        "start": "d2-app-scripts start",
        "build": "d2-app-scripts build",
        "test": "d2-app-scripts test",
        "coverage": "npm run test --coverage",
        "cypress:start": "BROWSER=none yarn start",
        "cy:open-live": "cypress_dhis2_api_stub_mode=DISABLED d2-utils-cypress open --appStart 'yarn cypress:start'",
        "cy:run-live": "cypress_dhis2_api_stub_mode=DISABLED d2-utils-cypress run --appStart 'yarn cypress:start'",
        "cy:open-stub": "d2-utils-cypress open --tag @non-mutating --appStart 'yarn cypress:start'",
        "cy:run-stub": "d2-utils-cypress run --tag @non-mutating --appStart 'yarn cypress:start'",
        "cy:capture": "cypress_dhis2_api_stub_mode=CAPTURE yarn d2-utils-cypress run --appStart 'yarn cypress:start'"
    },
    "jest": {
        "transformIgnorePatterns": [
            "node_modules/(?!(lodash-es|@dhis2/d2-ui-[a-z-]+)/)"
        ],
        "setupFilesAfterEnv": [
            "../../config/testSetup.js"
        ]
    }
}
