        ">0.5%",
        "last 2 versions",
        "Firefox ESR",
        "ie 11",
        "not dead"
    ],
    "dependencies": {
        "@dhis2/d2-i18n": "^1.0.4",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
